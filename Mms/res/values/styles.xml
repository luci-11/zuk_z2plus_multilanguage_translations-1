<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="confirmdialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/dialog_transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">false</item>
    </style>
    <style name="TextAppearance">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimary</item>
        <item name="android:textColorHighlight">#ffff9200</item>
        <item name="android:textColorHint">?android:textColorHint</item>
        <item name="android:textColorLink">#ff5c5cff</item>
    </style>
    <style name="TextAppearance.SearchResult" parent="@style/TextAppearance">
        <item name="android:textStyle">normal</item>
        <item name="android:textColor">?android:textColorPrimaryInverse</item>
        <item name="android:textColorHint">?android:textColorHintInverse</item>
    </style>
    <style name="TextAppearance.SearchResult.Title" parent="@style/TextAppearance.SearchResult">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="TextAppearance.SearchResult.Subtitle" parent="@style/TextAppearance.SearchResult">
        <item name="android:textSize">13.0sp</item>
        <item name="android:textColor">?android:textColorSecondaryInverse</item>
    </style>
    <style name="MmsHoloTheme" parent="@android:style/Theme.Holo.Light.DarkActionBar" />
    <style name="WidgetTitle" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Title">
        <item name="android:textColor">#ff333333</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="NotificationPrimaryText">
        <item name="android:textColor">#ffcccccc</item>
    </style>
    <style name="NotificationSecondaryText">
        <item name="android:textColor">#ff666666</item>
    </style>
    <style name="NotificationSubjectText">
        <item name="android:textColor">#ff999999</item>
    </style>
    <style name="TextShadowLight">
        <item name="android:textColor">@color/solid_white</item>
        <item name="android:shadowColor">@color/text_shadow_color_light</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">2.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="BannerTitleText" parent="@style/TextShadowLight">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="BannerContentText" parent="@style/TextShadowLight">
        <item name="android:textSize">14.0sp</item>
        <item name="android:ellipsize">end</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="MmsNewStyleTheme" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:spinnerDropDownItemStyle">@style/CustomDropDownItemStyle</item>
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:actionModeBackground">@color/action_mode_background</item>
        <item name="android:actionModeCloseDrawable">@drawable/back_black</item>
        <item name="android:actionOverflowButtonStyle">@style/OverFlow</item>
        <item name="android:actionMenuTextAppearance">@style/MMSTextAppearance.Holo.Widget.ActionBar.Menu</item>
        <item name="android:actionBarWidgetTheme">@style/MyActionBarWidgetTheme</item>
        <item name="android:colorPrimary">@color/default_actionabr_background</item>
        <item name="android:colorPrimaryDark">@color/primary_color_dark</item>
    </style>
    <style name="MmsSettingStyleTheme" parent="@android:style/Theme.Material.Light.DarkActionBar">
        <item name="android:spinnerDropDownItemStyle">@style/CustomDropDownItemStyle</item>
        <item name="android:actionBarStyle">@style/MyActionBar</item>
        <item name="android:actionModeBackground">@color/action_mode_background</item>
        <item name="android:actionModeCloseDrawable">@drawable/back_black</item>
        <item name="android:actionOverflowButtonStyle">@style/OverFlow</item>
        <item name="android:actionMenuTextAppearance">@style/MMSTextAppearance.Holo.Widget.ActionBar.Menu</item>
        <item name="android:actionBarWidgetTheme">@style/MyActionBarWidgetTheme</item>
        <item name="android:colorPrimary">@color/default_actionabr_background</item>
        <item name="android:colorPrimaryDark">@color/primary_color_dark</item>
        <item name="android:colorAccent">@color/default_actionabr_background</item>
    </style>
    <style name="MMSTextAppearance.Holo.Widget.ActionBar.Menu" parent="@android:style/TextAppearance.Holo.Widget.ActionBar.Menu">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="CustomDropDownItemStyle" parent="@android:style/Widget.Holo.DropDownItem.Spinner">
        <item name="android:textAppearance">@style/CustomDropDownItemTextStyle</item>
    </style>
    <style name="CustomDropDownItemTextStyle">
        <item name="android:textSize">18.0sp</item>
    </style>
    <style name="MyActionBarWidgetTheme" parent="@android:style/Theme.Material.Light">
        <item name="android:searchResultListItemHeight">61.0dip</item>
    </style>
    <style name="MyActionBar" parent="@android:style/Widget.Material.Light.ActionBar">
        <item name="android:background">#ff0288d1</item>
        <item name="android:titleTextStyle">@style/ActionBarTitleTextStyleTheme</item>
        <item name="android:subtitleTextStyle">@style/ActionBarSubTitleTextStyleTheme</item>
    </style>
    <style name="ActionBarTitleTextStyleTheme" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Title">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:fontFamily">sans-serif-medium</item>
    </style>
    <style name="ActionBarSubTitleTextStyleTheme" parent="@android:style/TextAppearance.Material.Widget.ActionBar.Subtitle">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="OverFlow" parent="@android:style/Widget.Holo.ActionButton.Overflow">
        <item name="android:src">@drawable/more</item>
    </style>
    <style name="MyActionButton" parent="@android:style/Widget.ActionButton">
        <item name="android:gravity">center</item>
        <item name="android:paddingRight">16.0dip</item>
        <item name="android:scaleType">center</item>
        <item name="android:minWidth">24.0dip</item>
    </style>
    <style name="HeaderTheme" parent="@android:style/ThemeOverlay.Material.Dark.ActionBar">
        <item name="android:colorBackground">@android:color/background_material_light</item>
        <item name="android:colorAccent">?android:textColorPrimary</item>
        <item name="android:popupTheme">@android:style/ThemeOverlay.Material.Light</item>
    </style>
    <style name="NoActionBar" parent="@android:style/Theme.Material.Light.NoActionBar">
        <item name="android:colorBackground">@android:color/background_material_light</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:actionButtonStyle">@style/MyActionButton</item>
        <item name="android:actionModeBackground">@color/action_mode_background</item>
        <item name="android:actionModeCloseDrawable">@drawable/back_black</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
    <style name="QuickReplyTheme" parent="@android:style/Theme.Material.NoActionBar.Fullscreen">
        <item name="android:windowBackground">@color/quick_reply_color</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="SelectableCopyTheme" parent="@android:style/Theme.Material.Light.NoActionBar.Fullscreen">
        <item name="android:windowBackground">@color/white</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:colorAccent">@color/full_screen_accent_color</item>
    </style>
    <style name="RecipientEditTextViewBase" parent="@android:attr/autoCompleteTextViewStyle">
        <item name="android:textAppearance">?android:textAppearanceMedium</item>
        <item name="android:background">@null</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minHeight">@dimen/chip_height</item>
        <item name="android:popupBackground">@android:color/white</item>
        <item name="android:inputType">textEmailAddress|textMultiLine</item>
        <item name="android:textDirection">locale</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="RecipientEditTextView" parent="@style/RecipientEditTextViewBase">
        <item name="android:imeOptions">actionNext|flagNoFullscreen</item>
    </style>
    <style name="ChipAutocompleteWrapperStyle">
        <item name="android:paddingLeft">@dimen/chip_wrapper_start_padding</item>
        <item name="android:paddingTop">@dimen/chip_wrapper_top_padding</item>
        <item name="android:paddingRight">@dimen/chip_wrapper_end_padding</item>
        <item name="android:paddingBottom">@dimen/chip_wrapper_bottom_padding</item>
    </style>
    <style name="ChipAutocompleteDividerStyle">
        <item name="android:background">@color/autocomplete_divider_color</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/chip_custom_divider_height</item>
        <item name="android:layout_marginLeft">@dimen/chip_wrapper_start_padding</item>
    </style>
    <style name="ChipAutocompleteDividerStyle.PermissionItem" parent="@style/ChipAutocompleteDividerStyle">
        <item name="android:background">@drawable/chip_autocomplete_divider_background</item>
        <item name="android:layout_height">3.0dip</item>
        <item name="android:layout_marginLeft">0.0dip</item>
    </style>
    <style name="ChipTextViewLayoutStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:orientation">vertical</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="ChipTitleStyle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/chips_dropdown_text</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:id">@android:id/title</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="ChipSubtitleStyle">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/chips_dropdown_text</item>
        <item name="android:ellipsize">middle</item>
        <item name="android:id">@android:id/text1</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:singleLine">true</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="ChipIndicatorTextStyle" parent="@style/ChipSubtitleStyle">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_marginTop">6.0dip</item>
        <item name="android:drawablePadding">4.0dip</item>
    </style>
    <style name="ChipPermissionStyle">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/chips_dropdown_permission_text</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:text">@string/chips_permission_text</item>
        <item name="android:layout_weight">1.0</item>
        <item name="android:textAlignment">viewStart</item>
    </style>
    <style name="ChipIconBaseStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:id">@android:id/icon</item>
        <item name="android:layout_width">40.0dip</item>
        <item name="android:layout_height">40.0dip</item>
        <item name="android:contentDescription">@null</item>
    </style>
    <style name="ChipStartIconStyle" parent="@style/ChipIconBaseStyle">
        <item name="android:layout_marginRight">@dimen/chip_icon_margin_end</item>
    </style>
    <style name="ChipEndIconStyle" parent="@style/ChipIconBaseStyle">
        <item name="android:layout_marginLeft">@dimen/chip_icon_margin_end</item>
    </style>
    <style name="ChipDeleteIconBaseStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:id">@android:id/icon1</item>
        <item name="android:paddingTop">10.0dip</item>
        <item name="android:paddingBottom">10.0dip</item>
        <item name="android:layout_width">36.0dip</item>
        <item name="android:layout_height">48.0dip</item>
    </style>
    <style name="ChipDeleteIconStyle" parent="@style/ChipDeleteIconBaseStyle">
        <item name="android:paddingLeft">@dimen/chip_icon_padding</item>
    </style>
    <style name="ChipPermissionDismissIconStyle">
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:id">@android:id/icon2</item>
        <item name="android:background">?android:selectableItemBackground</item>
        <item name="android:padding">@dimen/chip_icon_padding</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:src">@drawable/chips_ic_close_24dp</item>
        <item name="android:contentDescription">@android:string/cancel</item>
    </style>
</resources>
